{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
          "src": "/assets/images/(.*)",
          "destination": "/src/assets/images/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/index.html"
        }
    ]
}
{
    "version": 2,
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/dist/(.*)",
        "dest": "/dist/$1"
      },
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
